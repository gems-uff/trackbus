<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.uff.trackbus" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Trackbus</name>
  <description>
        Trackbus
    </description>
  <author email="you@example.com" href="http://example.com.com/">
        Your Name Here
    </author>
  <content src="index.html"/>
  <access origin="http://dadosabertos.rio.rj.gov.br/apiTransporte/apresentacao/rest/index.cfm/*"/>
