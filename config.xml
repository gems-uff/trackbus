  <plugin name="cordova-plugin-vibration" spec="~2.1.2"/>
  <plugin name="cordova-sqlite-storage" spec="~1.4.6"/>
  <plugin name="cordova-plugin-file" spec="~4.3.0"/>
  <plugin name="cordova-plugin-background-mode" spec="https://github.com/katzer/cordova-plugin-background-mode.git"/>
  <plugin name="cordova-plugin-tts" spec="~0.2.3"/>
